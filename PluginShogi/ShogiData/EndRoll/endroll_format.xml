<?xml version="1.0" encoding="UTF-8"?>

<FormatRoot>
	<MovieTimeline FadeInStartTime="00:00:05"
				   FadeInSpan="00:00:05"
				   FadeOutStartTime="00:01:28"
				   FadeOutSpan="00:00:05" />
	<EndRollTimeline FadeInStartTime="00:00:10"
					 FadeInSpan="00:00:00"
					 FadeOutStartTime="00:01:32"
					 FadeOutSpan="00:00:00" />

	<ColumnDefinitions>
		<ColumnDefinition Width="1" />
		<ColumnDefinition Width="1" />
	</ColumnDefinitions>

	<Line>
		<Format ColumnSpan="2" Text="参加者一覧" />
	</Line>

	<Lines Data="$JoinedVoterList">
		<Line>
			<Format Column="0" Text="$Name" Alignment="Right" />
			<Format Column="1" Text="$Skill" Alignment="Left" />
		</Line>
	</Lines>

	<Line />
	<Lines Data="$UnjoinedVoterCount">
		<Line>
			<Format Column="0" Text="その他" Alignment="Right" />
			<Format Column="1" Text="$()人" Alignment="Left" />
		</Line>
	</Lines>

	<Line />
	<Line />
	<Line>
		<Format ColumnSpan="2" Text="クジラちゃん参加ＰＣ一覧" />
	</Line>
	
	<Lines Data="$ModeCustomJoinerList">
		<Line>
			<Format ColumnSpan="2"
					Text="$" Alignment="Center" />
		</Line>
	</Lines>

	<Line />
	<Line />
	<Line>
		<Format ColumnSpan="2" Text="放送主一覧" />
	</Line>
	
	<Lines Data="$LiveOwnerList">
		<Line>
			<Format ColumnSpan="2"
					Text="$Name" Color="$Color" Alignment="Center" />
		</Line>
	</Lines>
	
	<Line />
	<Line />
	<Line>
		<Format ColumnSpan="2" Text="最後までおつきあい頂き" />
	</Line>
	<Line>
		<Format ColumnSpan="2" Text="どうもありがとうございました" />
	 </Line>
</FormatRoot>
